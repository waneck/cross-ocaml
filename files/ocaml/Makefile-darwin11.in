# generated by ./configure 
PREFIX=/usr/x86_64-apple-darwin11/
BINDIR=$(PREFIX)/bin
LIBDIR=$(PREFIX)/lib/ocaml
STUBLIBDIR=$(LIBDIR)/stublibs
MANDIR=$(PREFIX)/man
MANEXT=1
RANLIB=x86_64-apple-darwin11-ranlib
CROSS=x86_64-apple-darwin11-
RANLIBCMD=ranlib
ARCMD=x86_64-apple-darwin11-ar
SHARPBANGSCRIPTS=true
BNG_ARCH=amd64
BNG_ASM_LEVEL=1
PTHREAD_LINK=-cclib -lpthread
X11_INCLUDES=not found
X11_LINK=not found
TK_DEFS=
TK_LINK=
LIBBFD_LINK=
BYTECC=clang
BYTECCCOMPOPTS= -fno-defer-pop -Wall -D_FILE_OFFSET_BITS=64 -D_REENTRANT
BYTECCLINKOPTS=
BYTECCLIBS= -lm  -ldl  -lpthread
BYTECCRPATH=
EXE=
SUPPORTS_SHARED_LIBRARIES=true
SHAREDCCCOMPOPTS=
MKSHAREDLIBRPATH=
NATDYNLINKOPTS=
SYSLIB=-l$(1)
#ml let syslib x = "-l"^x;;

### How to build a static library
MKLIB=x86_64-apple-darwin11-ar rcs $(1) $(2); x86_64-apple-darwin11-ranlib $(1)
#ml let mklib out files opts = Printf.sprintf "x86_64-apple-darwin11-ar rcs %s %s %s; x86_64-apple-darwin11-ranlib %s" out opts files out;;
ARCH=amd64
MODEL=default
SYSTEM=macosx
NATIVECC=x86_64-apple-darwin11-clang
NATIVECCCOMPOPTS= -D_FILE_OFFSET_BITS=64 -D_REENTRANT
NATIVECCPROFOPTS= -D_FILE_OFFSET_BITS=64 -D_REENTRANT
NATIVECCLINKOPTS=
NATIVECCRPATH=
NATIVECCLIBS=  -lm  -ldl
ASM=x86_64-apple-darwin11-clang -arch x86_64 -c
ASPP=x86_64-apple-darwin11-clang -arch x86_64 -c
ASPPPROFFLAGS=-DPROFILING
PROFILING=prof
DYNLINKOPTS=
OTHERLIBRARIES=unix str num dynlink bigarray systhreads threads
DEBUGGER=ocamldebugger
CC_PROFILE=-pg
SYSTHREAD_SUPPORT=true
PARTIALLD=x86_64-apple-darwin11-ld -r -arch x86_64
PACKLD=$(PARTIALLD) $(NATIVECCLINKOPTS) -o\ 
DLLCCCOMPOPTS=
IFLEXDIR=
O=o
A=a
SO=so
EXT_OBJ=.o
EXT_ASM=.s
EXT_LIB=.a
EXT_DLL=.so
EXTRALIBS=
CCOMPTYPE=cc
TOOLCHAIN=cc
NATDYNLINK=true
CMXS=cmxs
MKEXE=x86_64-apple-darwin11-$(BYTECC) -Wl,-no_compact_unwind
MKEXEDEBUGFLAG=-g
MKDLL=x86_64-apple-darwin11-clang -bundle -flat_namespace -undefined suppress -Wl,-no_compact_unwind
MKMAINDLL=clang -bundle -flat_namespace -undefined suppress -Wl,-no_compact_unwind
RUNTIMED=noruntimed
CAMLP4=camlp4
ASM_CFI_SUPPORTED=true
WITH_FRAME_POINTERS=false
